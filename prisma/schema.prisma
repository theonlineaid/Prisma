generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @default(autoincrement())
  email String @unique
  name  String @map("firstName")
  role  String?

  @@map("users")

  @@id([email,name])
  @@unique([name, role])
  @@index([id,email])
}
